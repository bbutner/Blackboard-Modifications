{
  "manifest_version": 2,

  "name": "Blackboard Modifications",
  "description": "CSS Website Manipulation",
  "version": "1.0",

  "web_accessible_resources": [
    "header_institution.png"
  ],

  "browser_action": {
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://dacc.blackboard.com/*", "https://dacc.blackboard.com/*"],
      "js": ["jquery.js", "main.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
